graph TD
    A(("Начало"))
    B[/"Ввод: points, x0"/]
    COEF["coef = InterpolationMethodsBase.dividedDifferences(points)"]
    C["result = 0"]
    D{{"1 <= i < points.size"}}
    E["term = coef[k]"]
    F{{"0 <= j < k"}}
    G["term *= (x0 - points[j].x)"]
    H["result += term"]
    I[/"Возврат result"/]
    J(("Конец"))

    A --> B --> COEF --> C --> D
    D -- true --> E --> F -- true --> G --> F -- false --> H --> D
    D -- false --> I --> J